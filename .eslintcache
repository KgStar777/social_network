W </button>\r\n\r\n                                : <button disabled={props.followingInProgress.some(id => id === u.id)}\r\n                                          className={classes.unFollowBtn}\r\n                                          onClick={() => { props.follow(u.id);\r\n                                          }}> FOLLOW </button>\r\n                            }\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            )\r\n        }\r\n        {/*<button className={classes.getBtn} onClick={this.getUsers}>GET USERS</button>*/}\r\n    </div>\r\n}\r\n\r\nexport default Users","C:\\Users\\User\\Desktop\\Code\\JS\\JavaScript\\React\\React Camasutra\\react-project\\src\\components\\common\\Preloader\\Preloader.js",["181"],"import loading_2 from \"../../../assets/images/svg/loader/loading_2.gif\";\r\nimport React from \"react\";\r\n\r\nlet Preloader = () => {\r\n    return (\r\n        <div style={{\r\n            width: '100%',\r\n            display: 'flex'\r\n        }}>\r\n            <img src={loading_2}\r\n                 style={{\r\n                     width: '50px',\r\n                     margin: '0 auto'\r\n                 }}/>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Preloader","C:\\Users\\User\\Desktop\\Code\\JS\\JavaScript\\React\\React Camasutra\\react-project\\src\\components\\Profile\\ProfileContainer.jsx",["182"],"C:\\Users\\User\\Desktop\\Code\\JS\\JavaScript\\React\\React Camasutra\\react-project\\src\\redux\\authReducer.js",[],"C:\\Users\\User\\Desktop\\Code\\JS\\JavaScript\\React\\React Camasutra\\react-project\\src\\components\\Header\\HeaderContainer.jsx",[],"C:\\Users\\User\\Desktop\\Code\\JS\\JavaScript\\React\\React Camasutra\\react-project\\src\\api\\api.js",[],"C:\\Users\\User\\Desktop\\Code\\JS\\JavaScript\\React\\React Camasutra\\react-project\\src\\components\\Login\\Login.jsx",["183"],"import React from 'react';\r\nimport classes from \"./Login.module.css\";\r\nim